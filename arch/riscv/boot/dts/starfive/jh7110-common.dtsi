	ov5640: ov5640@3c {
		compatible = "ovti,ov5640";
		reg = <0x3c>;
		clocks = <&clk_ext_camera>;
		clock-names = "xclk";
		//DOVDD-supply = <&v2v8>;
		rotation = <180>;
		port {
			// Parallel bus endpoint
			ov5640_to_parallel: endpoint {
				remote-endpoint = <&parallel_from_ov5640>;
				bus-type = <5>;		/* Parallel */
				bus-width = <8>;
				data-shift = <2>;	/* lines 13:6 are used */
				hsync-active = <0>;
				vsync-active = <1>;
				pclk-sample = <1>;
			};
		};
	};

	sc2235: sc2235@30 {
		compatible = "sc2235";
		reg = <0x30>;
		clocks = <&clk_ext_camera>;
		clock-names = "xclk";

		port {
			/* Parallel bus endpoint */
			sc2235_to_parallel: endpoint {
				remote-endpoint = <&parallel_from_sc2235>;
				bus-type = <5>;      /* Parallel */
				bus-width = <8>;
				data-shift = <2>; /* lines 13:6 are used */
				hsync-active = <1>;
				vsync-active = <1>;
				pclk-sample = <1>;
			};
		};
	};
};

