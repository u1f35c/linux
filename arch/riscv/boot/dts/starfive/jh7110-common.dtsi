	ov4689: ov4689@36 {
		compatible = "ovti,ov4689";
		reg = <0x36>;
		clocks = <&clk_ext_camera>;
		clock-names = "xclk";
		//reset-gpio = <&gpio 18 0>;
		rotation = <180>;

		port {
			/* Parallel bus endpoint */
			ov4689_to_csi2rx0: endpoint {
				remote-endpoint = <&csi2rx0_from_ov4689>;
				bus-type = <4>;		/* MIPI CSI-2 D-PHY */
				clock-lanes = <0>;
				data-lanes = <1 2 3 4>;
			};
		};
	};

	ov13850@10 {
		compatible = "ovti,ov13850";
		reg = <0x10>;
		clocks = <&clk_ext_camera>;
		clock-names = "xclk";
		//reset-gpio = <&gpio 18 0>;
		rotation = <0>;

		port {
			/* CSI2 bus endpoint */
			ov13850_to_csi2rx0: endpoint {
				remote-endpoint = <&csi2rx0_from_ov13850>;
				bus-type = <4>;      /* MIPI CSI-2 D-PHY */
				clock-lanes = <2>;
				data-lanes = <0 1>;
			};
		};
	};
};
/* default sd card */
&sdio0 {
	clock-frequency = <102400000>;
	max-frequency = <200000000>;
	card-detect-delay = <300>;
	bus-width = <4>;
	broken-cd;
	cap-sd-highspeed;
	post-power-on-delay-ms = <200>;
	pinctrl-names = "default";
	pinctrl-0 = <&sdcard0_pins>;
	//cd-gpios = <&gpio 23 0>;
	status = "okay";
};

&sdio1 {
	clock-frequency = <4000000>;
	max-frequency = <1000000>;
	card-detect-delay = <300>;
	bus-width = <4>;
	cap-sd-highspeed;
	cap-sdio-irq;
	cap-mmc-hw-reset;
	non-removable;
	enable-sdio-wakeup;
	keep-power-in-suspend;
	cap-mmc-highspeed;
	post-power-on-delay-ms = <200>;
	pinctrl-names = "default";
	pinctrl-0 = <&mmc1_pins>;
	status = "okay";
};

&vin_sysctl {
	/* when use dvp open this pinctrl*/
	//pinctrl-names = "default";
	//pinctrl-0 = <&dvp_pins>;
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@2 {
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;

			/* Parallel bus endpoint */
			parallel_from_ov5640: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&ov5640_to_parallel>;
				bus-type = <5>;		/* Parallel */
				bus-width = <8>;
				data-shift = <2>;	/* lines 9:2 are used */
				hsync-active = <1>;
				vsync-active = <0>;
				pclk-sample = <1>;
				status = "failed";
			};

			/* Parallel bus endpoint */
			parallel_from_sc2235: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&sc2235_to_parallel>;
				bus-type = <5>;		/* Parallel */
				bus-width = <8>;
				data-shift = <2>;	/* lines 9:2 are used */
				hsync-active = <1>;
				vsync-active = <0>;
				pclk-sample = <1>;
				status = "okay";
			};
		};

		port@3 {
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;

			/* CSI2 bus endpoint */
			csi2rx0_from_ov13850: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&ov13850_to_csi2rx0>;
				bus-type = <4>;      /* MIPI CSI-2 D-PHY */
				clock-lanes = <0>;
				data-lanes = <1 2 3 4>;
				status = "failed";
			};

			/* CSI2 bus endpoint */
			csi2rx0_from_ov4689: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&ov4689_to_csi2rx0>;
				bus-type = <4>;      /* MIPI CSI-2 D-PHY */
				clock-lanes = <0>;
				data-lanes = <1 2 3 4>;
				status = "okay";
			};
		};
	};
};

